open Console as cmd


// higher-order Closures... functions example
fn run(v: i32, f: fn(i32)i32): i32 -> f(v)

fn main() {
    var x: i32 = 50

    fn add_x: i32(y: i32) -> x + y
    fn square: i32(num: i32) -> num ^ 2

    cmd.print(add_x(run(5, square)))
    // output: 75
}


fn main() {
    val nums = [1, 2, 3, 4, 5, 6]
    nums.for_each(x -> cmd.print("${x}: hi"))
    // also map filter is fp features
}
