open Console as cmd


// higher-order Closures... functions example
run:(v i32, f (i32)i32)i32 -> f(v)

main: {
    mut x := i32(50)

    add_x:(y i32)i32 -> x + y
    square:(num i32)i32 -> num ^ 2

    cmd.print(add_x(run(5, square)))
    // output: 75
}


main: {
    nums := [1, 2, 3, 4, 5, 6]
    nums.for_each((x) -> cmd.print("${x}: hi"))
    // also map filter is fp features
}
