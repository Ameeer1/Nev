open nev:cli

main :: () {
  numbers := [1, 2, 3, 4, 5, 6]
  numbers.for_each x => cli.print("${x}: hi": line)

  doubled_numbers = numbers
    |> filter x => x % 2 == 0
    |> map x => x *2

  cli.print(doubled_numbers: word) // 4 8 12
  // also map filter is fp features
}

